{
    "launch": {

      "configurations": [
        {
          "type": "mix_task",
          "name": "start phoenix server",
          "request": "launch",
          "projectDir": "${workspaceRoot}",
          "task": "phx.server"
        }
      ],
      "compounds": []
    },
    "explorer.autoReveal": false,
    "editor.fontFamily": "JetBrains Mono",
    "editor.fontSize": 14,
    "editor.fontWeight": "bold",
    "editor.fontLigatures": true,
    "explorer.confirmDragAndDrop": false,
    "files.autoSave": "afterDelay",
    "terminal.integrated.copyOnSelection": true,
    "vsicons.dontShowNewVersionMessage": true,
    "trailing-spaces.highlightCurrentLine": false,
    "security.workspace.trust.untrustedFiles": "open",
    "explorer.confirmDelete": false,
    "elixirLS.suggestSpecs": false,
    "editor.tabSize": 2,
    "workbench.iconTheme": "vscode-icons",
    "editor.minimap.enabled": false,
    "editor.wordWrap": "on",
    "vim.easymotion": true,
    "vim.incsearch": true,
    "vim.useSystemClipboard": true,
    "vim.useCtrlKeys": true,
    "vim.hlsearch": true,
    "vim.insertModeKeyBindings": [
      {
        "before": ["j", "k"],
        "after": ["<Esc>"]
      }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
      {
        "before": ["<leader>", "d"],
        "after": ["d", "d"]
      },
      {
        "before": ["<C-n>"],
        "commands": [":nohl"]
      },
      {
        "before": ["K"],
        "commands": ["lineBreakInsert"],
        "silent": true
      }
    ],
    "vim.leader": "<space>",
    "vim.handleKeys": {
      "<C-a>": false,
      "<C-f>": false
    },
    "window.zoomLevel": 1,
    "editor.unicodeHighlight.allowedLocales": {
      "ru": true
    },
    "plpgsqlLanguageServer.definitionFiles": [
      "**/*.psql",
      "**/*.pgsql",
      "**/*.sql"
    ],
    "plpgsqlLanguageServer.port": 10432,
    "plpgsqlLanguageServer.database": "ucell_dev",
    "plpgsqlLanguageServer.defaultSchema": "ucell",
    "plpgsqlLanguageServer.user": "ucell_dev",
    "update.mode": "none",
    "extensions.autoCheckUpdates": false,
    "workbench.colorTheme": "Elixir Theme No Italics Less Dark 2"
}
